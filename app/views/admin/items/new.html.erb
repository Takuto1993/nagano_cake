<h3>商品新規登録画面</h3>
 <%= form_with model: @item, url: '/admin/items' do |f| %>
   <h4>商品画像</h4>
    <%= f.file_field :image, accept: "image/" %>
   <h4>商品名</h4>
    <%= f.text_field :name %>
   <h4>商品説明分</h4>
    <%= f.text_area :introduction %>
   <h4>税抜き価格</h4>
    <%= f.text_field :price %><p>円</p>
   <%= f.submit '商品新規登録' %>
 <% end %>
